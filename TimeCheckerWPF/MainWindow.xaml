<Window x:Class="TimeCheckerWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TimeCheckerWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas x:Name="Canvas" HorizontalAlignment="Left" Height="419" VerticalAlignment="Top" Width="792" Loaded="Canvas_Loaded">
            <ListBox x:Name="ListBox" Height="225" Canvas.Left="407" Canvas.Top="80" Width="195"/>
            <TextBox x:Name="TextBox" Height="211" Canvas.Left="84" TextWrapping="Wrap" Text="TextBox" Canvas.Top="80" Width="198"/>
            <Button x:Name="BTextBox" Content="Button" Canvas.Left="133" Canvas.Top="30" Width="92" Height="37" Click="BTextBox_Click"/>
            <Button x:Name="BListBox" Content="Button" Canvas.Left="464" Canvas.Top="30" Width="87" Height="37" Click="BListBox_Click"/>
        </Canvas>

    </Grid>
</Window>
