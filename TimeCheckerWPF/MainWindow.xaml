<Window x:Class="TimeCheckerWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TimeCheckerWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="450">

    <Grid Loaded="Grid_Loaded">
        <StackPanel Orientation="Horizontal" Height="50" VerticalAlignment="Top" HorizontalAlignment="Left">
            <Button Content="Home" MinWidth="100" Click="Button_Click1" FontSize="24" FontWeight="Bold" FontFamily="Berlin Sans FB" Background="#FFCDA246"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal" Height="50" VerticalAlignment="Top" HorizontalAlignment="Right">
            <Button x:Name="Show" Content="Show" MinWidth="100" Click="Button_Show" HorizontalAlignment="Right" FontSize="24" FontWeight="Bold" FontFamily="Snap ITC" Background="#FF7DAEDE" />
        </StackPanel>
        <Frame x:Name="Main" Margin="0,50,0,0" NavigationUIVisibility="Hidden"/>

    </Grid>
</Window>
