<Window x:Class="TimeCheckerWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TimeCheckerWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="CheckInOut">
        <Canvas>
            <Button x:Name="CheckInOut1" Content="Read" Canvas.Left="42" Canvas.Top="28" Height="120" Width="194" BorderThickness="1,1,1,1" Click="CheckInOut1_Click"/>
            <TextBox x:Name="TextBox_Read" Canvas.Left="414" Canvas.Top="28" Text="TextBox" TextWrapping="Wrap" Width="299" Height="120" TextChanged="TextBox_TextChanged"/>
            <TextBox x:Name="TextBox_Send" Canvas.Left="414" Canvas.Top="196" Text="TextBox" TextWrapping="Wrap" Width="299" Height="120" TextChanged="TextBox_TextChanged"/>
            <Button x:Name="CheckInOut2" Content="Send" Canvas.Left="42" Canvas.Top="207" Height="99" Width="194" Click="Button_Click"/>
        </Canvas>

    </Grid>
</Window>
