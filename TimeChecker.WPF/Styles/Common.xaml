<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Colors-->
    <Color x:Key= "ColorPrimary1">#FF008000 </Color>
    <Color x:Key= "ColorPrimary2">#FF4682B4 </Color>
    <Color x:Key= "ColorPrimary3">#FFC71585</Color>

    <!--Brushes-->
    <SolidColorBrush x:Key="BrushPrimary1" Color="{StaticResource ColorPrimary1}"/>
    <SolidColorBrush x:Key="BrushPrimary2" Color= "{StaticResource ColorPrimary2}"/>
  

</ResourceDictionary>